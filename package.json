{
  "name": "schmaker",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "prebuild:tm": "node scripts/embed-assets.mjs",
    "build:tm": "node scripts/build-tm.mjs",
    "build:web": "vite build",
    "build": "npm run clean && npm run build:tm && npm run build:web && node -e \"require('fs').copyFileSync('dist/tm/schmaker.user.js','dist/web/schmaker.user.js')\"",
    "dev:web": "vite",
    "dev": "npm run build:tm && npm run dev:web",
    "preview:web": "vite preview",
    "preview:build": "npm run build && npm run preview:web",
    "parse:html": "node scripts/parse-html-cli.mjs"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.2",
    "jsdom": "^28.1.0",
    "lucide-react": "^0.575.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
